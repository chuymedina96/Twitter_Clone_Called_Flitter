<!DOCTYPE html>
<html>
<head>
  <title>Flitterv2</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render '/layouts/nav' %>
<div class="container">
       <spam class="notice"><%= notice %></spam>
       <spam class="alert"><%= alert %></spam>
<%= yield %>
</div>

</body>
</html>
